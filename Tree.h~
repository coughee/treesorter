#ifndef TREE_H_INCLUDE
#define TREE_H_INCLUDE

#include "Node.h"
#include <vector>
class Tree {
public:
  double length, radius, x, y, z;
  int p, c1, c2;
  bool isEndNode;
  
  std::vector<Node> nodes;
  
  Tree(){};
  
  void swap(int i, int j);
  int size();
  void readTree(char* filename);
  void writeTreeFile(char* outputname);
};

#endif
